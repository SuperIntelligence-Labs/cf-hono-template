    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "typescript-eslint": "^8.39.1",
    "wrangler": "^4.4.0",
    "jiti": "^2.5.1"
  },
  "dependencies": {
    "hono": "^4.9.1",
    "neverthrow": "^8.2.0",
    "tslog": "^4.9.3",
    "zod": "^4.0.17"
